


TAGS="";
NL=$(echo -e '\n')

function bump_inc(){
    
    release auto inc betainc SourceRoot ../ VersionStandard  beta-semantic
    TAG=$(git for-each-ref refs/tags --sort=-taggerdate --format "tag:%(refname:strip=2), %(subject)" --count=1)
    TAGS=$TAGS$NL$TAG
    git nugcast
    
}

PROJECTS=$(cat .publishes) 



for PROJECT in $PROJECTS
do
    (cd $PROJECT && bump_inc)
done


echo $TAGS