


function bump_inc(){
    
    release auto inc betainc SourceRoot ../ VersionStandard  beta-semantic
    nugcast
    
}

PROJECTS=$(cat .publishes) 

COUNT=0;

for PROJECT in $PROJECTS
do
    (cd $PROJECT && bump_inc)
    COUNT=$((COUNT+1));
done


git for-each-ref refs/tags --sort=-taggerdate --format "tag:%(refname:strip=2), %(subject)" --count=$COUNT